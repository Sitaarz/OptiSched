{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqLiteConnectionString": "Data Source=app.db;"
  },
  "AppSettings": {
    "JWTsecret": "ASecretKeyMustHaveAtLeast32CharactersToBeSecure"
  },
  "GeneticSettings": {
    "PopulationSize": 1000,
    "GenerationNumber": 100,
    "PercentageOfPopulationToMove": 0.2,
    "CrossoverProbability": 0.8,
    "MinutesRangeToMoveDuringMutation": 30
  }
}
